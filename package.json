{
  "name": "ngx-countdown",
  "version": "2.0.2",
  "main": "./bundles/ngx-countdown.umd.js",
  "typings": "index.d.ts",
  "description": "Simple, easy and performance countdown for angular",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipchk/ngx-countdown.git"
  },
  "keywords": [
    "ngx-countdown",
    "ng2-countdown",
    "angular-countdown",
    "angular2-countdown",
    "countdown",
    "count down"
  ],
  "author": "cipchk <cipchk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cipchk/ngx-countdown/issues"
  },
  "homepage": "https://github.com/cipchk/ngx-countdown#readme",
  "scripts": {
    "demo.gh-pages": "run-s demo.build demo.deploy",
    "demo.deploy": "gh-pages -d demo/dist",
    "demo.build": "ng build -prod",
    "link": "ngm link -p src --here",
    "lint": "exit 0",
    "build": "ngm build -p src --clean",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "pretest": "run-s lint build link",
    "test": "ng test -sr",
    "test-coverage": "ng test -sr -cc",
    "clean:tmp": "rimraf .ng_build .ng_compiled",
    "clean:tmp:lib": "npm run clean:tmp && rimraf .lib",
    "build:copy-sources": "gulp copy-sources",
    "build:inline-resources": "gulp inline-resources",
    "build:ts": "tsc -p tsconfig.publish.json && ngc -p tsconfig.publish.json",
    "build:bundle": "gulp bundle",
    "build:package": "npm-run-all -s clean:tmp:lib build:copy-sources build:ts build:inline-resources build:bundle clean:tmp",
    "version:bump": "gulp bump",
    "release:prepare": "npm run build:package",
    "release:validity": "npm run version:bump && npm run release:prepare",
    "release": "npm run version:bump && npm run release:prepare && npm publish --access=public .lib"
  },
  "dependencies": {},
  "peerDependencies": {
    "@angular/common": ">=4.3.x || >5.x",
    "@angular/core": ">=4.3.x || >5.x",
    "@angular/compiler": ">=4.3.x || >5.x",
    "typescript": ">=2.3.x"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/webpack": "^3.0.14",
    "codelyzer": "~3.2.0",
    "core-js": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.8.0",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.2.0",
    "gulp-cli": "^1.3.0",
    "gulp-connect": "^5.0.0",
    "gulp-dom": "^0.9.17",
    "gulp-flatten": "^0.3.1",
    "gulp-if": "^2.0.2",
    "gulp-markdown": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.0",
    "gulp-rollup": "^2.15.0",
    "gulp-sass": "^3.1.0",
    "gulp-transform": "^1.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngm-cli": "^0.8.3",
    "ngx-bootstrap": "^2.0.0-beta.7",
    "ngx-highlight-js": "^1.0.2",
    "ngx-notify": "^1.0.2",
    "npm-run-all": "^4.1.1",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rxjs": "^5.5.5",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.14"
  }
}
