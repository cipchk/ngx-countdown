    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngm-cli": "^0.8.3",
    "ngx-bootstrap": "^2.0.0-beta.7",
    "ngx-highlight-js": "^1.0.2",
    "ngx-notify": "^1.0.2",
    "npm-run-all": "^4.1.1",
    "protractor": "~5.1.2",
    "rxjs": "^5.5.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.14"
  }
